  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pqmcgill/jukebot/issues"
  },
  "homepage": "https://github.com/pqmcgill/jukebot#readme",
  "dependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "devDependencies": {
    "react-hot-loader": "^1.3.0"
  }
}
