{
  "name": "jukebot",
  "version": "1.0.0",
  "description": "A party music app for everyone!",
  "main": "App.js",
  "scripts": {
    "start": "node server",
    "postinstall": "webpack -p --config webpack.production.config.js",
    "dev": "webpack-dev-server --devtool eval --progress --colors --hot",
    "deploydev": "heroku config:set SOURCE_MAPS=\"true\" && git push heroku master",
    "deployprod": "heroku config:unset SOURCE_MAPS && git push heroku master"
  },
  "engines": {
    "node": "0.12.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pqmcgill/jukebot.git"
  },
  "author": "Patrick McGill",
  "license": "ISC",
  "homepage": "https://github.com/pqmcgill/jukebot#readme",
  "dependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "body-parser": "^1.15.0",
    "css-loader": "^0.23.1",
    "expose-loader": "^0.7.1",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "firebase": "^2.3.2",
    "history": "^1.13.1",
    "html-webpack-plugin": "^2.7.2",
    "imports-loader": "^0.6.5",
    "jquery": "^2.1.4",
    "node-sass": "^3.4.2",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.3",
    "react-router": "^2.0.0",
    "reactfire": "^0.6.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "webpack-dev-server": "^1.14.0",
    "react-hot-loader": "^1.3.0"
  }
}
